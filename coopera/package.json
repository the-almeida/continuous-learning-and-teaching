{
  "name": "coopera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services:up && next dev --turbopack",
    "services:up": "docker compose -f infra/docker-compose.yml up -d",
    "services:down": "docker compose -f infra/docker-compose.yml down",
    "services:stop": "docker compose -f infra/docker-compose.yml stop",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "next": "15.5.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^30.2.0",
    "typescript": "^5"
  }
}
